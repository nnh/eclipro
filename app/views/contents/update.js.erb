 $('#section-<%= @content.no.gsub(".", "-") %>-icon').html('<i class="fa fa-<%= @content.status_icon%>">');

<% if flash[:notice].present? %>
  toastr.success('<%= j(flash[:notice]) %>')
  <% flash[:notice] = nil %>
<% end %>
<% if flash[:alert].present? %>
  toastr.error('<%= j(flash[:alert]) %>')
  <% flash[:alert] = nil %>
<% end %>
