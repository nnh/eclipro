$('#section-<%= @content.no.gsub(".", "-") %>-editor').html(
  '<%= j(render "protocols/editor_form", protocol: @protocol, content: @content) %>');
$('#section-<%= @content.no.gsub(".", "-") %>-icon').html('<i class="fa fa-<%= @content.status_icon%>">');

tinymce.remove('#section-<%= @content.no.gsub(".", "-") %>-tinymce');
initTinyMCE('#section-<%= @content.no.gsub(".", "-") %>-tinymce');

<% if flash[:notice].present? %>
  toastr.success('<%= j(flash[:notice]) %>')
  <% flash[:notice] = nil %>
<% end %>
<% if flash[:alert].present? %>
  toastr.error('<%= j(flash[:alert]) %>')
  <% flash[:alert] = nil %>
<% end %>
